= semantic_form_for @user do |form|
  = form.inputs do
    =form.input :email
    =#form.input :trained, :label => "Trained?"
    =#form.input :retired, :label => "Retired?"
    =#form.input :creator, :label => "Can create EA's?"
    =#form.input :locked, :label => 'Locked?'
    =#form.input :password
    =#form.input :password_confirmation
  = form.buttons
